{
  "name": "@prolex/google-workspace-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Google Workspace (Sheets, Docs, Drive) - Prolex AI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tsc --noEmit",
    "docker:build": "docker build -t prolex/google-workspace-mcp-server:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "keywords": [
    "mcp",
    "google-workspace",
    "google-sheets",
    "google-docs",
    "google-drive",
    "automation",
    "typescript",
    "claude-desktop",
    "prolex"
  ],
  "author": "ProlexAi",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "googleapis": "^144.0.0",
    "google-auth-library": "^9.14.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "yaml": "^2.8.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^22.10.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
