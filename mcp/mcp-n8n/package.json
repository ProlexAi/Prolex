{
  "name": "@prolex/n8n-mcp-server",
  "version": "5.0.0",
  "description": "MCP server for n8n workflow automation - v5 with security, autonomy management, and self-healing",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "watch": "tsx watch src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --testPathPattern=e2e",
    "lint": "tsc --noEmit",
    "migrate": "tsx scripts/migrate-from-v4.ts",
    "docker:build": "docker build -t prolex/n8n-mcp-server:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down"
  },
  "keywords": [
    "mcp",
    "n8n",
    "workflow",
    "automation",
    "typescript",
    "claude-desktop"
  ],
  "author": "ProlexAi",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "axios": "^1.7.9",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "pg": "^8.13.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "yaml": "^2.8.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
